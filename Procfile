web: java $JAVA_OPTS -Ddw.server.connector.port=$PORT -Ddw.mongo.seeds[0].host=$MONGODB_HOST -Ddw.mongo.seeds[0].port=$MONGODB_PORT -Ddw.mongo.database=$MONGODB_DATABASE -Ddw.mongo.credentials.userName=$MONGODB_USERNAME -Ddw.mongo.credentials.password=$MONGODB_PASSWORD -jar target/juleluka-api-0.1.0-SNAPSHOT.jar server config-heroku.yml